model ContactForm {
  id      Int     @id @default(autoincrement())
  name    String
  email   String
  message String
  createdAt DateTime @default(now())
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  body      String
  author    String   @default("")
  createdAt DateTime @default(now())
}
